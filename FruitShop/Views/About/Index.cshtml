@using FruitShop.Models.Classes
@model List<FruitShop.Models.About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Admin_LayOut.cshtml";
}
<br />
<table class="table table-bordered">
        
    <tr>
        <th>Title</th>  
        <th>Subtitle</th>
       
        <th>Delete</th>
        <th>Update</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.Title</td>
            <td>@x.SubTitle</td>
           
            <td><a class="btn btn-danger" href="/About/DeleteAbout/@x.ID">Delete</a></td>
            <td><a class="btn btn-primary" href="/About/ShowAbout/@x.ID">Update</a></td>
        </tr>
    }
</table>
<a href="/About/AddAbout" class="btn btn-default">Add New Info</a>